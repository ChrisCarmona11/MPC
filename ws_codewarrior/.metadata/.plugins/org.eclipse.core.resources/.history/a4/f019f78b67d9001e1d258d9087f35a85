#include "derivative.h" /* include peripheral declarations */
#include <stdio.h>

FILE *fprt;

int main(void)
{
	int counter = 0;
	filePointer = fopen("constantes_int_1.h","r");
	char *content;
	char *fileFinalPointer;
	fseek(fileFinalPointer, 0L, SEEK_END);
	int sz = ftell(fileFinalPointer);
	fgets(content,sz , fprt);
	printf(content);
	
	for(;;) {	   
	   	counter++;
	}
	
	return 0;
}
